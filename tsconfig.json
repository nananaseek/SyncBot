{
    "compilerOptions": {
      "sourceMap": true,
      "noImplicitAny": false,
      "esModuleInterop": false,
      "module": "esnext",
      "moduleResolution": "node",
      "target": "es6",
      "lib": [
        "es2015",
        "es2017",
        "ES2021",
        "dom"
      ],
      "removeComments": false,
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "jsx": "react",
      "allowJs": true,
      "baseUrl": ".",
      "paths": {
        "*": [
          "./frontend/src/*"
        ]
      },
    },
    "include": ["./frontend/src"]
  
  }